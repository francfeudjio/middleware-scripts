pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                sleep 5
            }
        }
        stage('plan') {
            steps {
                echo 'Hello plan'
                sleep 5
            }
        }
        stage('code') {
            steps {
                echo 'Hello code'
                sleep 5
            }
        }
        stage('Build') {
            steps {
                echo 'Hello Build'
                sleep 5
            }
        }
        stage('test') {
            steps {
                echo 'Hello test'
                sleep 5
            }
        }
        stage('deployment') {
            steps {
                echo 'Hello deployment'
                sleep 5
            }
        }
    }
}
